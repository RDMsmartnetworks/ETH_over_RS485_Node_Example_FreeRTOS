#Generating source browse information for project Project


#Abbreviations
cc = C$:\Program$ Files$ (x86)\IAR$ Systems\Embedded$ Workbench$ 8.3\common\bin\SourceIndexer.exe
ll = C$:\Program$ Files$ (x86)\IAR$ Systems\Embedded$ Workbench$ 8.3\common\bin\PbdLink.exe
bd = C$:\Program$ Files$ (x86)\IAR$ Systems\Embedded$ Workbench$ 8.3\common\bin\makeBrowseData.exe



#Rules
rule index
  depfile = $out.dep
  command = $cc -out=$out -f $in
rule link
  command = $ll -M $out $in
rule browsedata
  command = $bd $in -output  $out



#Build steps
build system_stm32f4xx.pbi : index system_stm32f4xx.xcl
build stm32f4xx_crc.pbi : index stm32f4xx_crc.xcl
build stm32f4xx_rcc.pbi : index stm32f4xx_rcc.xcl
build stm32f4xx_rng.pbi : index stm32f4xx_rng.xcl
build main.pbi : index main.xcl
build stm32f4xx_it.pbi : index stm32f4xx_it.xcl
build Project_part0.pbi : link system_stm32f4xx.pbi stm32f4xx_crc.pbi
build Project_part1.pbi : link stm32f4xx_rcc.pbi stm32f4xx_rng.pbi
build Project_part2.pbi : link main.pbi stm32f4xx_it.pbi
build Project.pbd : link Project_part0.pbi Project_part1.pbi Project_part2.pbi
build Project.pbw : browsedata Project.pbd


